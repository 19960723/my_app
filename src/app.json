      {
        "text": "热门",
        "pagePath": "pages/hot/main",
        "iconPath": "static/tabs/tab_popular_no.png",
        "selectedIconPath": "static/tabs/tab_popular_yes.png"
      },
      {
        "text": "我的",
        "pagePath": "pages/center/main",
        "iconPath": "static/tabs/tab_my_no.png",
        "selectedIconPath": "static/tabs/tab_my_yes.png"
      }
